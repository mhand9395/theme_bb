            "liquid": "<style>\n  .countdown-container {\n    text-align: center;\n    font-family: 'Segoe UI', sans-serif;\n    margin-top: 20px;\n  }\n\n  .countdown-title {\n    font-size: 18px;\n    font-weight: 900;\n    margin-bottom: 10px;\n    letter-spacing: 1px;\n  }\n\n  .countdown-blocks {\n    display: flex;\n    justify-content: center;\n    gap: 12px;\n  }\n\n  .countdown-block {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    gap: 4px;\n  }\n\n  .countdown-value {\n    background-color: #38bdf8; /* bleu ciel */\n    color: white;\n    font-weight: bold;\n    font-size: 28px;\n    padding: 2px 16px;\n    border-radius: 12px;\n    min-width: 60px;\n    text-align: center;\n  }\n\n  .countdown-label {\n    font-size: 12px;\n    font-weight: bold;\n    color: #111827; /* noir */\n    text-transform: uppercase;\n  }\n</style>\n\n<div class=\"countdown-container\">\n  <div class=\"countdown-title\">DERNIERE CHANCE POUR PROFITER DES REDUCTIONS !</div>\n  <div class=\"countdown-blocks\" id=\"countdownTimer\">\n    <div class=\"countdown-block\">\n      <div class=\"countdown-value\" id=\"cd-hours\">--</div>\n      <div class=\"countdown-label\">Hours</div>\n    </div>\n    <div class=\"countdown-block\">\n      <div class=\"countdown-value\" id=\"cd-minutes\">--</div>\n      <div class=\"countdown-label\">Minutes</div>\n    </div>\n    <div class=\"countdown-block\">\n      <div class=\"countdown-value\" id=\"cd-seconds\">--</div>\n      <div class=\"countdown-label\">Seconds</div>\n    </div>\n  </div>\n</div>\n\n<script>\n  function startRepeatingCountdown() {\n    const hEl = document.getElementById('cd-hours');\n    const mEl = document.getElementById('cd-minutes');\n    const sEl = document.getElementById('cd-seconds');\n\n    function updateCountdown() {\n      const now = new Date();\n      const totalCycle = 8 * 60 * 60; // 8h = 28800s\n      const minCycle = 2 * 60 * 60;   // 2h = 7200s\n      const cycleRange = totalCycle - minCycle; // 21600s\n\n      const secondsSinceEpoch = Math.floor(now.getTime() / 1000);\n      const secondsInCycle = secondsSinceEpoch % cycleRange;\n      let remaining = totalCycle - secondsInCycle;\n\n      if (remaining < minCycle) {\n        remaining = totalCycle;\n      }\n\n      const hours = Math.floor(remaining / 3600);\n      const minutes = Math.floor((remaining % 3600) / 60);\n      const seconds = Math.floor(remaining % 60);\n\n      hEl.textContent = String(hours).padStart(2, '0');\n      mEl.textContent = String(minutes).padStart(2, '0');\n      sEl.textContent = String(seconds).padStart(2, '0');\n    }\n\n    updateCountdown();\n    setInterval(updateCountdown, 1000);\n  }\n\n  document.addEventListener('DOMContentLoaded', startRepeatingCountdown);\n</script>"
